{
  "name": "leadid/simple-php-framework",
  "description": "Testing out a new leadid php framework.",
  "type": "library",
  "license": "MIT",
  "version": "1.0.6",
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "leadid\\simple_php_framework\\": "src/"
    }
  },
  "scripts": {
    "post-install-cmd": [
            "echo 'post-install' >> update_checker_status.txt",
      "leadid\\simple_php_framework\\UpdateChecker::execute"
    ],
    "pre-install-cmd": [
      "echo 'pre-install' >> update_checker_status.txt",
      "leadid\\simple_php_framework\\UpdateChecker::execute"
    ],
    "post-autoload-dump": [
      "echo 'autoload' >> update_checker_status.txt",
      "leadid\\simple_php_framework\\UpdateChecker::execute"
    ]
  }
}
